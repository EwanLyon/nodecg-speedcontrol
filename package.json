{
  "name": "nodecg-speedcontrol",
  "version": "2.0.0",
  "description": "NodeCG bundle to help facilitate the running of speedrunning marathons, including overlays.",
  "homepage": "https://github.com/speedcontrol/nodecg-speedcontrol#readme",
  "bugs": {
    "url": "https://github.com/speedcontrol/nodecg-speedcontrol/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/speedcontrol/nodecg-speedcontrol.git"
  },
  "license": "MIT",
  "author": "zoton2",
  "scripts": {
    "autofix": "run-s autofix:*",
    "autofix:browser": "npx eslint --fix --ext .ts,.vue,.html src/dashboard",
    "autofix:extension": "npx eslint --fix --ext .ts src/extension",
    "build": "run-s build:*",
    "build:browser": "trash .cache && cross-env NODE_ENV=production npm run bundle",
    "build:extension": "trash extension && npx tsc -b tsconfig.extension.json",
    "bundle": "trash dashboard && node script/bundle.js",
    "start": "node ../..",
    "typeschemas": "node script/typeschemas.js",
    "watch": "run-p watch:*",
    "watch:browser": "npm run bundle",
    "watch:extension": "trash extension && npx tsc -b tsconfig.extension.json -w"
  },
  "dependencies": {
    "clone": "^2.1.2",
    "express": "^4.17.1",
    "livesplit-core": "^0.10.2",
    "lodash": "^4.17.15",
    "markdown-it": "^9.0.1",
    "needle": "^2.4.0",
    "p-iteration": "^1.1.8",
    "parse-duration": "^0.1.1",
    "remove-markdown": "^0.3.0",
    "twitch-js": "^1.2.17",
    "uuid": "^3.3.2",
    "ws": "^7.1.2"
  },
  "devDependencies": {
    "@mdi/font": "^4.1.95",
    "@types/clone": "^0.1.30",
    "@types/lodash": "^4.14.136",
    "@types/markdown-it": "0.0.8",
    "@types/needle": "^2.0.4",
    "@types/node": "^12.7.2",
    "@types/remove-markdown": "^0.1.1",
    "@types/uuid": "^3.4.5",
    "@types/ws": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "@vue/component-compiler-utils": "^3.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-vue": "^5.2.3",
    "json-schema-to-typescript": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "trash-cli": "^3.0.0",
    "typescript": "^3.5.3",
    "vue": "^2.6.10",
    "vue-hot-reload-api": "^2.3.3",
    "vue-template-compiler": "^2.6.10",
    "vuedraggable": "^2.23.0",
    "vuetify": "^2.0.10",
    "vuex": "^3.1.1"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.esm.js"
  },
  "nodecg": {
    "compatibleRange": "^1.0.0",
    "dashboardPanels": [
      {
        "name": "run-player",
        "title": "Run Player",
        "file": "run-player.html",
        "width": 3
      },
      {
        "name": "run-editor-dash",
        "title": "Run Editor",
        "file": "run-editor-dash.html",
        "width": 3,
        "workspace": "Run Modifications"
      },
      {
        "name": "add-remove-runs-dash",
        "title": "Add/Remove Runs",
        "file": "add-remove-runs-dash.html",
        "width": 2,
        "workspace": "Run Modifications"
      },
      {
        "name": "horaro-schedule-import",
        "title": "Horaro Schedule Import",
        "file": "horaro-schedule-import.html",
        "width": 2,
        "workspace": "Run Modifications"
      },
      {
        "name": "timer",
        "title": "Timer",
        "file": "timer.html",
        "width": 2
      },
      {
        "name": "twitch-control",
        "title": "Twitch Control",
        "file": "twitch-control.html",
        "width": 3
      },
      {
        "name": "player-layout",
        "title": "Player Layout",
        "file": "player-layout.html",
        "width": 2,
        "workspace": "Scene Editing"
      },
      {
        "name": "run-modification-dialog",
        "title": "",
        "width": 5,
        "file": "run-modification-dialog.html",
        "dialog": true
      },
      {
        "name": "alert-dialog",
        "title": "",
        "width": 3,
        "file": "alert-dialog.html",
        "dialog": true
      }
    ]
  },
  "vue": {
    "runtimeCompiler": true
  }
}
